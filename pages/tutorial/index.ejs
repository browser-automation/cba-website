---
title: Tutorial
---

<%
tutorialList = site.queryPages(({originalPathname}) => {
return originalPathname && originalPathname.split('/')[0] == 'tutorial'}) %>
<ul>
<% for (const {title, pathname, originalPathname} of tutorialList) { 
    if (originalPathname !== 'tutorial') { %>
    <li><a href = '<%= pathname %>'><%= title %> (video)</a></li>
  <%  }
 } %>
</ul>